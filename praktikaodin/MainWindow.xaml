<Window x:Class="praktikaodin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:praktikaodin"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="61*"/>
            <RowDefinition Height="373*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button x:Name="DataSetButton" Grid.Column="0" FontSize="20" HorizontalContentAlignment="Left" Click="DataSetButton_Click">--DataSet</Button>
            <Button x:Name="EFButton" Grid.Column="1" FontSize="20" HorizontalContentAlignment="Right" Click="EFButton_Click">EF--</Button>
        </Grid>
        <Frame x:Name="PageFrame" NavigationUIVisibility="Hidden" Grid.Row="1"/>
    </Grid>
</Window>
